{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "default-case": "error",
        "no-console": "off",
        "no-var": "error",
        "prefer-const": "error",
        "yoda": "error",
        "semi": "error"
    }
}